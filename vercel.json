{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/dts/(.*)",
            "destination": "/src/index.ts"
        }
    ],
    "redirects": [
        {
            "source": "/",
            "destination": "/api/dts",
            "permanent": true
        }
    ]
}